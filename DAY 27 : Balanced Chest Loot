#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int T;
    cin >> T;

    while (T--) {
        int N;
        cin >> N;

        unordered_map<int, int> freq;
        for (int i = 0; i < N; i++) {
            int x;
            cin >> x;
            freq[x]++;
        }

        bool possible = true;
        for (auto &p : freq) {
            if (p.second % 2 != 0) {
                possible = false;
                break;
            }
        }

        cout << (possible ? "yes" : "no") << '\n';
    }

    return 0;
}
