#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    int t;
    cin >> t;

    while (t--) {
        int n;
        cin >> n;

        vector<vector<int>> grid(n, vector<int>(n));
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                cin >> grid[i][j];
            }
        }

        // Directions: Up, Down, Left, Right
        int dx[4] = {-1, 1, 0, 0};
        int dy[4] = {0, 0, -1, 1};

        vector<vector<bool>> visited(n, vector<bool>(n, false));
        queue<pair<int, int>> q;

        // Start from top-left corner
        q.push({0, 0});
        visited[0][0] = true;

        bool canEscape = false;

        while (!q.empty()) {
            auto [x, y] = q.front();
            q.pop();

            // If reached destination
            if (x == n - 1 && y == n - 1) {
                canEscape = true;
                break;
            }

            // Explore 4 directions
            for (int i = 0; i < 4; i++) {
                int nx = x + dx[i];
                int ny = y + dy[i];

                if (nx >= 0 && ny >= 0 && nx < n && ny < n &&
                    !visited[nx][ny] && grid[nx][ny] == 0) {
                    visited[nx][ny] = true;
                    q.push({nx, ny});
                }
            }
        }

        cout << (canEscape ? "YES\n" : "NO\n");
    }

    return 0;
}
