#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<pair<long long, long long>> v(n);

    for (int i = 0; i < n; i++) {
        cin >> v[i].first >> v[i].second;
    }

    // Sort intervals by start time
    sort(v.begin(), v.end());

    long long start = v[0].first;
    long long end = v[0].second;

    for (int i = 1; i < n; i++) {
        // If overlapping or touching
        if (v[i].first <= end) {
            end = max(end, v[i].second);
        } else {
            cout << start << " " << end << endl;
            start = v[i].first;
            end = v[i].second;
        }
    }

    // Print last interval
    cout << start << " " << end << endl;

    return 0;
}
