#include <iostream>
using namespace std;

long long gcd(long long a, long long b) {
    while (b != 0) {
        long long r = a % b;
        a = b;
        b = r;
    }
    return a;
}

int main() {
    long long A, B, T;
    cin >> A >> B >> T;

    long long g = gcd(A, B);

    if (T % g == 0)
        cout << "Yes";
    else
        cout << "No";

    return 0;
}
